{
	"name": "@rbxts/material-color-utilities",
	"version": "0.2.7",
	"publishConfig": {
		"access": "public"
	},
	"description": "Algorithms and utilities that power the Material Design 3 (M3) color system, including choosing theme colors from images and creating tones of colors; all in a new color space.",
	"keywords": [
		"material",
		"material design",
		"material3",
		"material you",
		"color extraction",
		"theming"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/simplynoni/roblox-material-color-utilities.git"
	},
	"author": "simpylnoni",
	"license": "Apache-2.0",
	"type": "module",
	"files": [
		"out",
		"!**/*.tsbuildinfo"
	],
	"main": "out/init.lua",
	"scripts": {
		"watch": "rbxtsc -w --verbose --rojo test.project.json --type model",
		"compile-package": "rbxtsc --verbose",
		"compile-model": "rbxtsc --verbose --rojo test.project.json --type model",
		"build-place": "rojo build test.project.json -o test.rbxl",
		"run-tests": "run-in-roblox --place test.rbxl --script scripts/run_tests.lua",
		"test": "npm run compile-model & npm run build-place & npm run run-tests"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.2",
		"@rbxts/compiler-types": "^2.3.0-types.1",
		"@rbxts/types": "^1.0.783",
		"roblox-ts": "^2.3.0",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@rbxts/jest": "^0.1.0",
		"@rbxts/jest-globals": "^0.1.0",
		"@rbxts/sift": "^0.0.9"
	}
}
